---
import CommonHead from "./CommonHead.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props as Props;
---

<!doctype html>
<html lang="en">
  <meta charset="UTF-8" />
  <CommonHead title={title} />
  <body class="bg-neutral-900 text-neutral-100">
    <slot />
  </body>
</html>
